<h1>New project</h1>

 <%= form_for(@project) do |f| %>
   
   <%= f.label :name %>
   <%= f.text_field :name %>
   
   <%= f.label :bio %>
   <%= f.text_field :bio %>
   
   <%= f.label :amount_raised %>
   <%= f.text_field :amount_raised %>   
   
   <%= f.label :amount_needed %>
   <%= f.text_field :amount_needed %>
   
   <%= f.submit %>
 <% end %>